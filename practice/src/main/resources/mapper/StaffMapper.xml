<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiao.boot.mapper.StaffMapper">
    <insert id="insertAddStaff">
        insert into practice2.staff
        values (#{staff2.jobId},#{staff2.name},null,#{staff2.sex},#{staff2.mobile},#{staff2.post},
                #{staff2.password},#{staff2.role},null,null,#{staff2.status})
    </insert>

</mapper>